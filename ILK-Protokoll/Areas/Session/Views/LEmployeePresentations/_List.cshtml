@using ILK_Protokoll.util
@model IEnumerable<ILK_Protokoll.Areas.Session.Models.Lists.EmployeePresentation>

<h3>Mitarbeiterpräsentationen</h3>

<table id="employeePresentation_table" class="table table-striped table-hover table-sortable">
	<thead>
		<tr>
			<th class="col-sm-2">
				@Html.DisplayNameFor(model => model.Employee)
			</th>
			<th class="col-sm-2">
				@Html.DisplayNameFor(model => model.Ilk)
			</th>
			<th class="col-sm-2">
				@Html.DisplayNameFor(model => model.Prof)
			</th>
			<th class="col-sm-2">
				@Html.DisplayNameFor(model => model.LastPresentation)
			</th>
			<th class="col-sm-2">
				@Html.DisplayNameFor(model => model.Attachments)
			</th>
			<th class="col-sm-1">
				@Html.DisplayNameFor(model => model.Selected)
			</th>
			<th class="col-sm-1"></th>
		</tr>
	</thead>
	<tbody id="employeePresentation_tbody">
		@foreach (var item in Model)
		{
			Html.RenderPartial("_Row", item);
		}
	</tbody>
	<tfoot>
		@Html.Action("_CreateForm", "LEmployeePresentations")
	</tfoot>
</table>

@using (Html.Delayed(null, "List_Utilities"))
{
	<script type="text/javascript" src="~/Scripts/ListUtilities.js"></script>
}