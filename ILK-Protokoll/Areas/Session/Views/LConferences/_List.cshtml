@model IEnumerable<ILK_Protokoll.Areas.Session.Models.Lists.Conference>

<h3>Auslandskonferenzen</h3>

<table class="table table-striped table-hover table-sortable">
	<thead>
		<tr>
			<th class="col-sm-2">
				@Html.DisplayNameFor(model => model.StartDate)
			</th>
			<th class="col-sm-2">
				@Html.DisplayNameFor(model => model.EndDate)
			</th>
			<th class="col-sm-3">
				@Html.DisplayNameFor(model => model.Description)
			</th>
			<th class="col-sm-1">
				@Html.DisplayNameFor(model => model.Ilk)
			</th>
			<th class="col-sm-1">
				@Html.DisplayNameFor(model => model.Employee)
			</th>
			<th class="col-sm-1">
				@Html.DisplayNameFor(model => model.Funding)
			</th>
			<th class="col-sm-1">
				@Html.DisplayNameFor(model => model.Approved)
			</th>
			<th class="col-sm-1"></th>
		</tr>
	</thead>
	<tbody id="conferencelist_body">
		@foreach (var item in Model)
		{
			Html.RenderPartial("_Row", item);
		}
	</tbody>
	<tfoot>
		@Html.Action("_CreateForm", "LConferences")
	</tfoot>
</table>